if (!global.frozen) &amp;&amp; (canDamage)
{
    //animate
    animTimer++;
    image_index = (animTimer / 10) mod 2;
    
    //decelerate lateral movement
    if abs(yspeed) != 0
        yspeed = (abs(yspeed) - 0.02) * (4 / 5) * sign(yspeed);
    //decelerate horizontal movement
    if moveSpeed &lt; 7
        moveSpeed += 0.35;
    else
        moveSpeed = 7;
    //return to default despawn range after being alive for a bit
    if animTimer &gt; 20
        despawnRange = 4;
    
    //move
    xspeed = dir * moveSpeed;
}
