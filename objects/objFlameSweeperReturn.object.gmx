            <string>///strong against mid-hp enemies, weak if detonated right on enemy

//especially effective against semi-bulky enemies
if (other.healthpointsStart &lt; 8)
    &amp;&amp; !object_is_ancestor(other.object_index, prtMiniBoss)
    global.damage = 3;

//hit counter copied from objTinTrooperExplosion, and modified a bit
//for each entry in the list of hit enemies...
var length = array_length_1d(hasHitList);
for (var g = 0; g &lt; length; g++)
{
    //if this enemy matches the entry, cancel the damage and do nothing else
    if other.id == hasHitList[g]
    {
        global.damage = -1;
        exit;
    }
}
//if we didn't get a match, do damage as normal, 
//but tell all of the fireballs to never hit this enemy again
//(assuming this is real early in our lifetime)
if animTimer &lt; 4
{
    var oid = other.id;
    with(object_index)
    {
        hasHitList[array_length_1d(hasHitList)] = oid;
        //print(hasHitList);
    }
}
