                {
                    for (var w = -4; w &lt;= 4; w += 4)
                    {
                        var f = instance_create(x, y, objFlameSweeperReturn);
                        f.dir = -sign(image_xscale);
                        f.image_xscale = -image_xscale;
                        f.yspeed = w;
                        if w == 0
                            f.moveSpeed = 2.30;
                        screenShake(10, 1, 1);
                    }
                    instance_create(x, y, objExplosion);
                    playSFX(sfxSolarBlazePopLarge);
                    instance_destroy();
                    exit;
                }
            }
        }
    }
}
