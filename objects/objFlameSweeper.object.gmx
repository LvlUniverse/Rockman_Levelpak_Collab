    {
        //sound
        playSFX(sfxCommandoBombLaunch);
        
        i.image_yscale = abs(image_yscale);
        //get an angle depending on the player's inputs
        if (xDir != 0)
        {
            i.moveAngle = -90 * xDir;
            i.image_xscale = xDir;
            var mxdir = xDir;
        }
        else
        {
            i.moveAngle = 90 * sign(-image_xscale)
            var mxdir = sign(image_xscale);
        }
        if (yDir != 0)
        {
            i.moveAngle += 25 * -mxdir * yDir;
            i.image_index = 1;
            if yDir == -1
                i.image_yscale = -i.image_yscale;
        }
        i.moveAngle = i.moveAngle + 90
    }
}
